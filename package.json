    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.7.5",
    "multer": "^1.4.5-lts.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "chai": "^4.5.0",
    "cross-env": "^10.1.0",
    "mocha": "^10.8.2",
    "nyc": "^17.1.0",
    "rimraf": "^6.1.0",
    "supertest": "^7.1.4"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "test/**"
    ],
    "extension": [
      ".js"
    ],
    "reporter": [
      "text-summary",
